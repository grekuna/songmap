<!DOCTYPE html>
<html>
  <head>
    <title>Songmap</title>
    <%= csrf_meta_tags %>


    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%# javascript_include_tag "//maps.google.com/maps/api/js?sensor=true", 'application', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>

    <script type="text/javascript"
     src="https://maps.googleapis.com/maps/api/js?key=<%=ENV['GOOGLEAPITOKEN']%>">
   </script>

  <%= favicon_link_tag 'musical-note.png', type: 'image/png' %>
  <link href="https://fonts.googleapis.com/css?family=Amatic+SC" rel="stylesheet">
  <link href='https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css' rel='stylesheet'>

  <meta property="og:title" content="Songmap" />
  <meta property="og:type" content="article" />
  <meta property="og:url" content="http://www.songmap.site/" />
  <meta property="og:image" content="/assets/logoname-edda7037d99d4c34e9d6cfce4549e48b30ea5ec0e3e597dd5ba70c11461f3da4.png" />
  <meta property="og:description" content="An interactice songmap to explore music and it's places" />

  </head>

  <body>
    <%= yield %>
  </body>
</html>
